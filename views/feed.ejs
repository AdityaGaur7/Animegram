   <main>
  <div class="container">
    
    <div class="gallery">
     <% posts.forEach(function(post){ %>
     <div class="gallery-item" tabindex="0">
            
         <img src="images/uploads/<%=post.image%>" class="gallery-image" alt="">
        
         
         
         <div class="gallery-item-info">
           

             <ul style="all:unset">
                 <li class="gallery-item-likes"><span class="visually-hidden">Likes:</span><i class="fas fa-heart" aria-hidden="true"></i> 89</li>
                 <li class="gallery-item-comments"><span class="visually-hidden">Comments:</span><i class="fas fa-comment" aria-hidden="true"></i> 5</li>
             </ul>

         </div>
         <h3 style="color: #333333;margin: auto;text-align: center;"><%=post.imageText%></h3>
         <h3 style="color:red;margin: auto;text-align: center;">Post by <%= post.user.fullname %></h3>
       
         

     </div>
     <%});%>
    </div>

 

 </div>
</main>
